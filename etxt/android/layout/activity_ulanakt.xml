<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ulanakt">
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:id="@+id/asakmenu2"
        app:menu="@menu/ulanyjymenu"
        android:layout_alignParentBottom="true"
        android:layout_height="wrap_content"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ulanyjy bölümi"
        android:textSize="16dp"
        android:textStyle="bold"
        android:fontFamily="@font/comfortaa_bold"
        android:layout_margin="5dp"/>
    <androidx.cardview.widget.CardView
        android:layout_alignParentRight="true"
        android:layout_margin="4dp"
        android:layout_width="wrap_content"
        app:cardCornerRadius="12dp"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="69dp"
            android:textAllCaps="false"
            android:id="@+id/closebutton"
            android:background="#FD1100"
            android:textSize="12dp"
            android:fontFamily="@font/audiowide"
            android:layout_height="29dp"
            android:text="Çykmak"/></androidx.cardview.widget.CardView>
    <androidx.fragment.app.FragmentContainerView
        android:layout_width="match_parent"
        android:id="@+id/suyereulanyjy"
        android:name="com.bbstudios.ecash.yumuslar.admin_ulanyjy"
        android:layout_above="@id/asakmenu2"
        android:layout_marginTop="35dp"
        android:layout_height="match_parent"/>
    <WebView
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/statweb"/>
</RelativeLayout>